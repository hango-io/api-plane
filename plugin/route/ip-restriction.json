{
  "inject": {
    "kind": "ip-restriction"
  },
  "layouts": [
    {
      "key": "type",
      "alias": "黑白名单类型",
      "type": "select",
      "options": [
        {
          "text": "黑名单",
          "value": "0"
        },
        {
          "text": "白名单",
          "value": "1"
        }
      ],
      "rules": [
        "Required",
        "Number"
      ]
    },
    {
      "key": "list",
      "alias": "ip列表",
      "default": [],
      "rules": [
        "Required",
        {"regex": "^((?:(?:[0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}(?:[0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(?:(\\/([1-9]|[1-2]\\d|3[0-1])))?)$|^(?:(?:[0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}(?:[0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])$"}
      ],
      "type": "multi_input"
    }
  ]
}